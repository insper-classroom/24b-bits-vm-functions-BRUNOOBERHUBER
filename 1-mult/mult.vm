// implemente a funcao mult que recebe dois argumentos
// e retorna a multiplicacao

function mult 2
    push constant 0
    pop local 0

    push argument 1
    pop local 1

label MULT_LOOP_START
    push local 1
    push constant 0
    eq
    if-goto MULT_LOOP_END

    push local 0
    push argument 0
    add
    pop local 0

    push local 1
    push constant 1
    sub
    pop local 1

    goto MULT_LOOP_START

label MULT_LOOP_END
    push local 0
    return
